{
  "version": 2,
  "builds": [
    {
      "src": "dist/elevenlabs-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mcp",
      "dest": "dist/elevenlabs-server.js"
    },
    {
      "src": "/health",
      "dest": "dist/elevenlabs-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 